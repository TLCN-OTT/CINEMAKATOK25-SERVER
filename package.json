{
  "name": "my-nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "worker:video": "ts-node -r tsconfig-paths/register libs/core/src/queue/video-worker.ts",
    "prettier": "prettier --write \"{src,apps,libs,test}/**/*.{ts,json,md}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node -r tsconfig-paths/register db/run-seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/cloudfront-signer": "^3.910.0",
    "@aws-sdk/lib-storage": "^3.910.0",
    "@elastic/elasticsearch": "^9.1.1",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/elasticsearch": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mau": "^0.2.4",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/passport-google-oauth20": "^2.0.16",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "config": "^4.1.0",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "ffmpeg-static": "^5.2.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.8.1",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "pixl-cache": "^1.0.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "latest",
    "@nestjs/testing": "^11.1.5",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^3.0.0",
    "@types/config": "^3.3.5",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.17.0",
    "@types/supertest": "^6.0.3",
    "copyfiles": "^2.4.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "passport-jwt": "^4.0.1",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1"
    }
  }
}
